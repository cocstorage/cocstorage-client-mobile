{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@pages": [
        "./pages"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@api": [
        "./src/api"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants": [
        "./src/constants"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@dto": [
        "./src/dto"
      ],
      "@dto/*": [
        "./src/dto/*"
      ],
      "@hooks": [
        "./src/hooks"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@library": [
        "./src/library"
      ],
      "@library/*": [
        "./src/library/*"
      ],
      "@recoil": [
        "./src/recoil"
      ],
      "@recoil/*": [
        "./src/recoil/*"
      ],
      "@styles": [
        "./src/styles"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "@emotion/react"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
